public class UserInfo implements Serializable
{
    /**
     * {变量说明}
     */
    private static final long serialVersionUID = 5402516802630861439L;

    // private static final Log LOGGER = LogFactory.getLog(UserInfo.class);

    private Integer id;

    private String name;

    private Integer age;

    public UserInfo()
    {

    }

    public UserInfo(Integer id, String name, Integer age)
    {
        this.id = id;
        this.name = name;
        this.age = age;
    }

    public String getName()
    {
        return name;
    }

    public void setName(String name)
    {
        this.name = name;
    }

    public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public Integer getAge() {
		return age;
	}

	public void setAge(Integer age) {
		this.age = age;
	}

	@Override
    public String toString()
    {
        return "UserInfo [id=" + id + ", name=" + name + ", age=" + age + "]";
    }

}
